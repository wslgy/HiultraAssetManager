<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/ll_assetBase"
        style="@style/default_ll" >

        <TextView
            style="@style/default_text"
            android:text="@string/title_base" />

        <TextView
            android:id="@+id/tv_base"
            style="@style/default_right_center"
            android:text="@string/text_click_clean" />

        <ImageView
            android:id="@+id/iv_base"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:background="@drawable/icon_del" />
    </LinearLayout>

    <View style="@style/default_devide" />

    <LinearLayout
